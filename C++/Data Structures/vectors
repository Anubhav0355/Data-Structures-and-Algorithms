#include<bits/stdc++.h>
using namespace std;
int main()
{
  vector<int>v;  //Declaring a vector of type 'int'and name 'v'
  
  v.push_back(1);  // so we insert values 1,3,8,23,99 in the vector.
  v.push_back(3);
  v.push_back(8);
  v.push_back(23);
  v.push_back(99);
  
  //To check whether our vector is filled or not.
  
  for(int i=0;i<v.size();i++)
   {
     cout<<v[i]<<" ";
   }
   cout<<"\n";
  
   cout<<v.size()<<"\n"; //printing the size of the vector.
   return 0;
}
